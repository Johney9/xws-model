                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="id_poruke_naloga" type="t:TID"/>
                <xs:element name="datum_valute" type="xs:date"/>
                <xs:element name="iznos" type="t:TIznos"/>
